    "@tanstack/react-query": "^5.51.11",
    "@types/lodash": "^4.17.7",
    "axios": "^1.7.2",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.1",
    "zustand": "^4.5.4"
